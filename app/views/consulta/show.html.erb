<div id="consulta" class="retrieve">
  <div class="banner-content">
    <%= show_svg("estetoscopio.svg") %>
  </div>

  <main>
    <header>
      <div class="profile">
        <%= link_to "Sair", destroy_user_session_path, method: :delete %>
        <%= link_to "Voltar", consulta_path %>
      </div>
      <img src="https://th.bing.com/th/id/R11cffbc044ed6560dc1b441d3ac9aeed?rik=VPbSjpc4MP4OrQ&riu=http%3a%2f%2fcdn.onlinewebfonts.com%2fsvg%2fdownload_169259.png&ehk=nVhocnKFtnrJBqcKSAfc9%2bpLn%2fJh6mkTyFnQV7A110U%3d&risl=&pid=ImgRaw" alt="profile img">
    </header>

    <div>
      <h1>
        Dados da consulta
        <%= link_to edit_consultum_path(@consultum), :class => "add-link" do %>
          <i class="large material-icons">class</i>
        <% end %>
      </h1>

      <div class="profile">
        <p>
          <strong>
            Veterinario:
          </strong>
          <%= @consultum.veterinario.nome %>
        </p>

        <p>
          <strong>
            Pet:
          </strong>
          <%= @consultum.animal.nome %>
        </p>

        <p>
          <strong>Sintomas:</strong>
          <%= @consultum.sintomas %>
        </p>

        <p>
          <strong>Prescricao:</strong>
          <%= @consultum.prescricao %>
        </p>

        <p>
          <strong>
            Data:
          </strong>
          <%= format_date(@consultum.data) %>
        </p>

        <p>
          <strong>
            Hora:
          </strong>
          <%= format_time(@consultum.data) %>
        </p>
      </div>
    </div>
  </main>
</div>
